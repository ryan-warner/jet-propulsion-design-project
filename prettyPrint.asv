function result = prettyPrint(stages)
    result = table();
    result.("TAmb") = [stages{1,1}.temperatureInitial]
    result.("TAmb") = [stages{1}.temperatureInitial]
    for stage = stages
        disp(stage{1}.station)
        result.(sprintf("T0%s",stage{1}.station)) = [stage{1}.temperatureFinal];
        result.(sprintf("P0%s",stage{1}.station)) = [stage{1}.temperatureFinal];
    end
end